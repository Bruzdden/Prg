#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    char delka[100];
    
    
    int count = 1;
    
    printf("%s", delka);
    scanf("%s", delka); 
    for(int i = 0; i < strlen(delka); i++){   

        if(delka[i] == delka[i + 1])
{
            count += 1;
}
        else
{
            printf("%c%d", delka[i], count); 
            count = 1;
}   
}
     return(0);
}
